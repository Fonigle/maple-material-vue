{
    "name": "maple-material-vue",
    "version": "0.5.1",
    "description": "Vue Component Library of Material Design 2",
    "main": "dist/maple-material-vue.js",
    "types": "types/index.d.ts",
    "scripts": {
        "iconfont": "cross-env TS_NODE_PROJECT='build/tsconfig.node.json' ts-node build/iconfont/iconfont-builder.ts",
        "webpack-dev": "cross-env MODE='development' TS_NODE_PROJECT='build/tsconfig.node.json' webpack-dev-server --config build/webpack/webpack.dev.config.ts",
        "webpack": "cross-env MODE='production' TS_NODE_PROJECT='build/tsconfig.node.json' webpack --config build/webpack/webpack.prod.config.ts --progress",
        "start": "npm run iconfont && npm run webpack-dev",
        "dist": "npm run webpack"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Fonigle/maple-material-vue.git"
    },
    "files": [
        "src",
        "dist",
        "types"
    ],
    "keywords": [
        "material",
        "vue"
    ],
    "author": "Fonigle",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Fonigle/maple-material-vue/issues"
    },
    "homepage": "https://github.com/Fonigle/maple-material-vue#readme",
    "dependencies": {
        "@babel/runtime": "7.5.5",
        "@material/button": "3.1.0",
        "@material/checkbox": "3.1.0",
        "@material/fab": "3.1.0",
        "@material/form-field": "3.1.0",
        "@material/icon-button": "3.1.0",
        "@material/list": "3.1.0",
        "@material/radio": "3.1.0",
        "@material/ripple": "3.1.0",
        "@material/switch": "3.1.0",
        "@material/textfield": "3.1.0"
    },
    "devDependencies": {
        "@babel/core": "7.5.5",
        "@babel/plugin-transform-runtime": "7.5.5",
        "@babel/preset-env": "7.5.5",
        "@types/friendly-errors-webpack-plugin": "0.1.2",
        "@types/html-webpack-plugin": "3.2.1",
        "@types/mini-css-extract-plugin": "0.8.0",
        "@types/node": "12.7.4",
        "@types/optimize-css-assets-webpack-plugin": "5.0.0",
        "@types/uglifyjs-webpack-plugin": "1.1.0",
        "@types/webpack": "4.39.1",
        "@types/webpack-dev-server": "3.1.7",
        "@types/webpack-merge": "4.1.5",
