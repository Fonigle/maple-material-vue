        "css-loader": "3.2.0",
        "file-loader": "4.2.0",
        "fork-ts-checker-webpack-plugin": "1.5.0",
        "friendly-errors-webpack-plugin": "1.7.0",
        "html-webpack-plugin": "3.2.0",
        "maple-theme": "2.1.0",
        "mini-css-extract-plugin": "0.8.0",
        "node-sass": "4.12.0",
        "optimize-css-assets-webpack-plugin": "5.0.3",
